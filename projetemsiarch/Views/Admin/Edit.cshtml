@model projetemsiarch.Models.Filiere

@{
    ViewData["Title"] = "Edit Filiere";
}

<h1>@ViewData["Title"]</h1>

<form asp-action="Edit" method="post">
    <div class="form-group">
        <label for="Nom">Nom</label>
        <input type="text" class="form-control" id="Nom" name="Nom" value="@Model.Nom" />
    </div>
    <button type="submit" class="btn btn-warning">Save Changes</button>
    <a href="@Url.Action("Index", "Filiere")" class="btn btn-secondary">Cancel</a>
</form>
